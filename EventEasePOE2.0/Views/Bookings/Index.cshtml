@model IEnumerable<EventEasePOE2._0.Models.Booking>

<h2>Bookings</h2>

<table>
    <thead>
        <tr><th>Event</th><th>Venue</th><th>Start</th><th>End</th></tr>
    </thead>
    <tbody>
        @foreach (var b in Model)
        {
            <tr>
                <td>@b.Event?.Name</td>
                <td>@b.Venue?.Name</td>
                <td>@b.StartDate.ToShortDateString()</td>
                <td>@b.EndDate.ToShortDateString()</td>
            </tr>
        }
    </tbody>
</table>
